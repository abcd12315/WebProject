@model EditLogModel
@{
    ViewData["Title"] = "EditLog";
}

<h1>EditLog</h1>

<form asp-route-returnUrl=@ViewData["ReturnUrl"]>
	<div class="form-group row">
		<label for="inputPassword" class="col-sm-2 col-form-label">Title</label>
		<div class="col-sm-10">
			<input type="text" class="form-control" id="inputPassword" asp-for="Title">
		</div>
	</div>

	<div class="form-group">
		<label for="exampleFormControlTextarea1">Content</label>
		<textarea class="form-control" asp-for="Content" rows="20" onkeydown="tab(this)"></textarea>
	</div>
	<input type="hidden" asp-for="Id" />


	<div class="form-group row">
		<div class="col-sm-9 " >
			enter button to submit
		</div>
		<div class="col-sm-3">
			<a href="javascript:history.go(-1)" class="btn btn-primary active" role="button" aria-pressed="true">back</a>

			<button type="submit" class="btn btn-dark">submit</button>
		</div>
	</div>
	
</form>
	
<script>
	function tab(obj) {
		if (event.keyCode == 9) {
			obj.value = obj.value + "    "; // 跳几格由你自已决定
			event.returnValue = false;
		}
	}
	$("asdsad").f
</script>